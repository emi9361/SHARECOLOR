<td><% if user_signed_in? %>
		<% if video.favorited_by?(current_user) %>
			<%= link_to video_favorites_path(video), method: :delete ,remote: true do %>
				<i class="fas fa-bookmark" aria-hidden="true" style="color: blue;"></i>
			<% end %>
	<% else %>
			<%= link_to video_favorites_path(video), method: :post, remote: true  do %>
				<i class="far fa-bookmark" aria-hidden="true" ></i>
			<% end %>
		<% end %>
	<% end %>
</td>