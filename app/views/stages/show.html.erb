<main class="stage-main">
<h1>Stage</h1>
		<a><%= link_to '戻る', stages_path, class: 'btn btn-info newpost_buttom', style: "text-alian:center;" ,data: {"turbolinks" => false} %></a>
		<a><%= link_to '編集', edit_stage_path, class: 'btn btn-info newpost_buttom', data: {"turbolinks" => false} %></a>
		<div class="stage-list">
			<div class="sound-write">
				<table class="table table-borderless sound-table">
					<thead>
						<tr>
						<%= render 'stage_favorite', stage: @stage %>
						</tr>
						<p><%= video_tag @stage.file,  class:"stage-sample", :controls => true %></p>
						</tbody>
				</table>
			</div>
			<%= link_to 'Tweet',"https://twitter.com/share?ref_src=twsrc%5Etfw", class:"twitter-share-button" , :data_show_count => "false"%>
		</div>
	</main>
		</div>
