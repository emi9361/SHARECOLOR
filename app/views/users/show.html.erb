<main class="user-main">
<h1>MyPage</h1>
<a><%= link_to '編集', edit_user_path(current_user), class: 'btn btn-info profile-edit', data: {"turbolinks" => false} %></a>
	<div class="user-list">
		<div class= "user-profile">
			<p><%= attachment_image_tag @user, :profile_image, format: 'jpeg' %></p>
			<p>Name : <% @user.name %></p>
			<p>Location : <% @user.location %></p>
			<p>Introduction <% @user.introduction %></p>
		</div>
	</div>
</main>
